# Copy this file to .env and fill in real values before starting the server
# MongoDB connection (replace with your DB URI)
MONGO_URI=mongodb://localhost:27017/iet-study

# JWT config
JWT_SECRET=please_change_this_to_a_secure_random_string
JWT_EXPIRE=7d

# Cookie expiration in days
COOKIE_EXPIRE=7

# Frontend origin used for CORS
FRONTEND_ORIGIN=http://localhost:5173

# Server port (optional)
PORT=5000
